#include <stdio.h>

int main()
{
    int num1,num2;
    
    printf("please enter the first number \n");
    scanf("%d",&num1);
    
    printf("please enter the second number \n");
    scanf("%d",&num2);
    
    if(num1>num2)
    {
        printf("%d is greater than %d",num1,num2);
    }
    else if(num1<num2)
    {
        printf("%d is less than %d",num1,num2);
    }
    else
    {
    printf("%d is equal to %d",num1,num2);
    }
    return 0;
}
